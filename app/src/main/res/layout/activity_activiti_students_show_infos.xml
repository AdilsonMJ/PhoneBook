<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.activity.activiti_students_show_infos"
    android:orientation="vertical">

    <TextView
        android:id="@+id/activity_student_info_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="78dp"
        android:padding="30dp"
        android:layout_margin="10dp"
        android:textSize="20sp"
        android:textAlignment="center" />

    <TextView
        android:id="@+id/activity_student_info_cel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/activity_student_info_email"
        android:layout_alignParentStart="true"
        android:layout_margin="10dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="10dp"
        android:padding="30dp"
        android:textAlignment="center"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/activity_student_info_email"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/activity_student_info_name"
        android:layout_alignParentStart="true"
        android:layout_marginStart="6dp"
        android:layout_marginTop="78dp"
        android:padding="30dp"
        android:layout_margin="10dp"
        android:textSize="20sp"
        android:textAlignment="center" />
</RelativeLayout>